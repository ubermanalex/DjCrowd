<form>

	<!-- Change "<li>test</li>" to whatever you want -->
	<!-- Change "0" to which list item to insert before (starts at 0, not 1) -->

	<input type='button' value="Gruppe erstellen" onClick="insertItem('thumbs',0)">

	<ul id="thumbs">

	</ul>

</form>

<script language="javascript">
	function insertItem(myid, position) {
		var groupname_input = prompt("Bitte Gruppennamen eingeben.")

		sock.onmessage = function(e) {
			console.log("message received: " + e.data);
			groupname = e.data;
			var ul = document.getElementById(myid);
			var li = document.createElement("li");
			li.innerHTML = '<li>' + groupname + '</li>';
			ul.insertBefore(li, ul.getElementsByTagName("li")[position]);
		}

		sock.send(groupname_input);
	}

</script>